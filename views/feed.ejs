<%- include ("./partials/header") %>

<div class="w-full min-h-screen bg-zinc-800  px-10 py-8 text-white">
<div class="cards flex flex-wrap gap-10">
    <% posts.forEach(element => { %>
        <div class="card">
            <div class="w-32 h-50 bg-sky-200 rounded-lg overflow-hidden">
                <img  class="w-full h-full object-cover" src="/images/uploads/<%= element.image %>" alt="">
            </div>
            <h5 class="mt-2 text-sm"><%= element.title %> </h5>
        </div>
    <% }) %>
 
</div>
</div>

<%- include ("./partials/footer") %>